    ["http://www.roblox.com/asset/?id=9648749145"] = "8",
    ["http://www.roblox.com/asset/?id=9648745618"] = "3",
    ["http://www.roblox.com/asset/?id=9648742013"] = "7",
    ["http://www.roblox.com/asset/?id=9648738553"] = "8",
